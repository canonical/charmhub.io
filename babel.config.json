{
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "prismjs",
      {
        "languages": ["python", "bash"]
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "modules": false
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"]
    }
  }
}
